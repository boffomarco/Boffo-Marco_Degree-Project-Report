@article{amador_robot_2019,
	title = {Robot {Localization} in an {Autonomous} {Ground} {Vehicle}},
	copyright = {http://www.cpp.edu/{\textasciitilde}broncoscholar/rightsreserved.html},
	url = {http://dspace.calstate.edu/handle/10211.3/207956},
	abstract = {Robot localization is the process by which robots ascertain information about their position and orientation relative to the external environment through the use of sensors, a competency which is necessary for autonomous navigation.  Previous research has demonstrated significant increases in the accuracy of localization estimates when GPS data is used in concert with that of odometry and inertial measurements units in a process which utilizes sensor fusion.  A common algorithm to fuse data from multiple sensors so as to reduce the uncertainty in state estimation is the unscented Kalman filter (UKF), part of a family of similar filters.  The current project compares the efficacy of multiple localization schemes, including the use of the UKF, to determine position of an autonomous robot as it navigates paths in an open-world environment.  Contrary to previous research, the current project finds that the related extended Kalman filter slightly outperforms the UKF in localization tasks.},
