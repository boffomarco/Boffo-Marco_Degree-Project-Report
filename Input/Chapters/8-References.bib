	note = {ISSN: 1050-4729},
	keywords = {SLAM, Particle, Kalman, Simulation},
	pages = {1985--1991 vol.2},
	file = {IEEE Xplore Full Text PDF:/home/marco/Zotero/storage/P5HECBXX/Montemerlo and Thrun - 2003 - Simultaneous localization and mapping with unknown.pdf:application/pdf}
}

